{
  "name": "mcp-supermarket-product-database",
  "version": "0.1.0",
  "description": "MCP server for managing supermarket product database",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "node --watch --experimental-strip-types src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "migrate": "node scripts/auto-migrate.js"
  },
  "keywords": [
    "mcp",
    "supermarket",
    "product",
    "database"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@supabase/supabase-js": "^2.49.8",
    "dotenv": "^16.5.0",
    "pg": "^8.16.0",
    "zod": "^3.25.42"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "prettier": "^3.5.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.33.0"
  }
}
