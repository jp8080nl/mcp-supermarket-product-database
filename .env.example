# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here

# Database Configuration (for migrations only - not used by MCP server)
SUPABASE_DB_PASSWORD=your-database-password-here

# Optional: Full connection string from Supabase Dashboard > Settings > Database
# SUPABASE_CONNECTION_STRING=postgresql://postgres.[project]:[password]@[host]:5432/postgres

# MCP Configuration
MCP_LOG_LEVEL=info

# Node Environment
NODE_ENV=development